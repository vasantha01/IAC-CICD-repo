{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Parameters": {
        "CompactionBucket": {
            "Type": "String",
            "Default": "compactios3bucket",
            "Description": "Enter Prism Bucket Name"
        }
    },
    "Resources": {
        "s3": {
            "Type": "AWS::CloudFormation::Stack",
            "Properties": {
                "TemplateURL": "https://s3bucket-poc1.s3.amazonaws.com/s3.json",
                "Parameters": {
                    "BucketName": {
                        "Ref": "CompactionBucket"
                    }
                },
                "TimeoutInMinutes": "60"
            }
        },
        "sns": {
            "Type": "AWS::CloudFormation::Stack",
            "Properties": {
                "TemplateURL": "https://s3bucket-poc1.s3.amazonaws.com/sns.json"    
            }
        }
    },
    "Outputs": {
        "StackRef": {
            "Value": {
                "Ref": "s3"
            }
        },
        "OutputFromNestedStack": {
            "Value": {
                "Fn::GetAtt": [
                    "s3",
                    "Outputs.BucketName"
                ]
            }
        }
    }
}
