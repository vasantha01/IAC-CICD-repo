{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Description": "Compaction Bucket Creation",
    "Parameters": {
        "bucketcicd": {
            "Type": "String",
            "Default": "compaction-bucket",
            "Description": "Enter Capsia Bucket Name"
        }
    },
    "Resources": {
        "S3Bucket": {
            "Type": "AWS::S3::Bucket",
            "Properties": {
                "BucketName": {
                    "Ref": "bucketcicd"
                }
            }
        }
    }
}
